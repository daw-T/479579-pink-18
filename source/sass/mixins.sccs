@mixin list-reset {
  margin: 0;
  padding: 0;
  list-style: none;
}

@mixin p-reset {
  margin: 0;
  padding: 0;
}

@mixin opensans-light {
  font-family: "Open-sans", "Arial", sans-serif;
  font-weight: 300;
  font-style: normal;
}

@mixin opensans-regular {
  font-family: "Open-sans", "Arial", sans-serif;
  font-weight: 400;
  font-style: normal;
}

@mixin opensans-bold {
  font-family: "Open-sans", "Arial", sans-serif;
  font-weight: 700;
  font-style: normal;
}
